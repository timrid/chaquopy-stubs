import java
import java.lang
import typing



class Effect(java.lang.Object):
    def __init__(self) -> None: ...
    def apply(self, arg1: int | java.jint | java.lang.Integer, arg2: int | java.jint | java.lang.Integer, arg3: int | java.jint | java.lang.Integer, arg4: int | java.jint | java.lang.Integer, /) -> None: ...
    def getName(self) -> str: ...
    def release(self) -> None: ...
    def setParameter(self, arg1: str | java.lang.String, arg2: java.lang.Object | int | bool | float | str, /) -> None: ...
    def setUpdateListener(self, listener: EffectUpdateListener, /) -> None: ...

class EffectContext(java.lang.Object):
    @staticmethod
    def createWithCurrentGlContext() -> EffectContext: ...
    def getFactory(self) -> EffectFactory: ...
    def release(self) -> None: ...

