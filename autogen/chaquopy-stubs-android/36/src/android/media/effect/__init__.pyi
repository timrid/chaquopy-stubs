    EFFECT_CONTRAST: typing.ClassVar[str] = ...
    EFFECT_CROP: typing.ClassVar[str] = ...
    EFFECT_CROSSPROCESS: typing.ClassVar[str] = ...
    EFFECT_DOCUMENTARY: typing.ClassVar[str] = ...
    EFFECT_DUOTONE: typing.ClassVar[str] = ...
    EFFECT_FILLLIGHT: typing.ClassVar[str] = ...
    EFFECT_FISHEYE: typing.ClassVar[str] = ...
    EFFECT_FLIP: typing.ClassVar[str] = ...
    EFFECT_GRAIN: typing.ClassVar[str] = ...
    EFFECT_GRAYSCALE: typing.ClassVar[str] = ...
    EFFECT_LOMOISH: typing.ClassVar[str] = ...
    EFFECT_NEGATIVE: typing.ClassVar[str] = ...
    EFFECT_POSTERIZE: typing.ClassVar[str] = ...
    EFFECT_REDEYE: typing.ClassVar[str] = ...
    EFFECT_ROTATE: typing.ClassVar[str] = ...
